{
    "name": "alchemy/phpexiftool",
    "type": "library",
    "description": "Exiftool driver for PHP",
    "keywords": ["metadata","exiftool"],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Neutron",
            "email": "imprec@gmail.com",
            "homepage": "http://www.lickmychip.com/"
        },
        {
            "name": "Benoit Burnichon",
            "email": "bburnichon@alchemy.fr",
            "role": "Lead Developer"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "exiftool/exiftool",
                "version": "12",
                "source": {
                    "url": "https://github.com/exiftool/exiftool.git",
                    "type": "git",
                    "reference": "12.42"
                }
            }
        }
    ],
    "require": {
        "php": "^7.4",
        "doctrine/cache": "^1.0",
        "doctrine/collections": "^1.0",
        "monolog/monolog": "^2.6",
        "exiftool/exiftool": "^12",
        "symfony/console": "^5",
        "symfony/process": "^5",
        "ext-dom": "*",
        "ext-memcache": "*",
        "cache/array-adapter": "^1.2"
    },
    "suggest": {
        "jms/serializer": "To serialize tags",
        "symfony/yaml": "To serialize tags in Yaml format"
    },
    "require-dev": {
        "jms/serializer": "~3",
        "phpunit/phpunit": "^5",
        "symfony/css-selector": "^5",
        "symfony/dom-crawler": "^5",
        "symfony/finder": "^5",
        "symfony/yaml": "^3"
    },
    "autoload": {
        "psr-4": {
            "PHPExiftool\\": "lib/PHPExiftool/"
        }
    }
}
